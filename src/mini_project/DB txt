--테이블 생성--

CREATE TABLE lottery_entries (
    id NUMBER PRIMARY KEY,
    user_id VARCHAR2(50),
    user_numbers VARCHAR2(50),
    winning_numbers VARCHAR2(50),
    result VARCHAR2(20)
);

--시스 생성--
CREATE SEQUENCE lottery_id_seq
START WITH 1
INCREMENT BY 1;

--전체 조회--
SELECT * FROM lottery_entries; 

--테이블 삭제--
DROP TABLE lottery_entries;
